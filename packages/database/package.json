{
  "name": "@athlete-system/database",
  "version": "1.0.0",
  "private": true,
  "description": "Database package with Prisma schema for Athlete Performance System",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts"
  },
  "scripts": {
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "build": "tsc",
    "clean": "rimraf dist",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0"
  },
  "devDependencies": {
    "prisma": "^5.7.0",
    "typescript": "^5.3.0",
    "tsx": "^4.6.0",
    "rimraf": "^5.0.0"
  },
  "keywords": [
    "athlete",
    "performance",
    "database",
    "prisma"
  ],
  "license": "MIT"
}
